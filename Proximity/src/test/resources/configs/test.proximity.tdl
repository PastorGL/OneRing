CREATE signals (@path='datasets/epsg4326/signals.csv');
TRANSFORM signals textToColumnar(@delimiter=',') VALUE (silos,userid,_,_,_,timestamp,lat,lon,_,_);
TRANSFORM signals columnarToPoint(@lat.column=lat,@lon.column=lon) POINT (silos,userid,lat,lon);

CREATE geometries (@path='datasets/epsg4326/points.json');
TRANSFORM geometries geoJsonToPoint(@"radius.default"=30000.);

CALL proximity INPUT points signals, pois geometries OUTPUT target target, evicted evicted;
