CREATE signals(@path='datasets/test.reach.audience');
TRANSFORM signals textToColumnar VALUE(userid);

CREATE target(@path='datasets/test.reach.signals');
TRANSFORM target textToColumnar(@delimiter=',') VALUE (gid,userid,_,_);

CALL reach(
@signals.userid.attr=userid,
@target.userid.attr=userid,
@target.grouping.attr=gid
) INPUT signals signals, target target OUTPUT result;
